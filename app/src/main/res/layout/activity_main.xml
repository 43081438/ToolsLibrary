<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="qzy.com.toolslibrary.MainActivity">

    <ListView
        android:id="@+id/swipe_target"
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:divider="@color/background"
        android:dividerHeight="1dp"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp"></ListView>

    <qzy.com.utilslib.svgText.VectorCompatTextView
        android:id="@+id/homePageIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="通讯录"
        android:textColor="#f84848"
        android:visibility="gone"
        app:drawableTopCompat="@drawable/svg_ic_txl"
        app:tintDrawableInTextColor="true"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />

</android.support.constraint.ConstraintLayout>
